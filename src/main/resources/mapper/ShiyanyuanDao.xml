<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.ShiyanyuanDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.ShiyanyuanEntity" id="shiyanyuanMap">
        <result property="zhanghao" column="zhanghao"/>
        <result property="mima" column="mima"/>
        <result property="xingming" column="xingming"/>
        <result property="xingbie" column="xingbie"/>
        <result property="touxiang" column="touxiang"/>
        <result property="youxiang" column="youxiang"/>
        <result property="dianhua" column="dianhua"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.ShiyanyuanVO" >
		SELECT * FROM shiyanyuan  shiyanyuan         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.ShiyanyuanVO" >
		SELECT  shiyanyuan.* FROM shiyanyuan  shiyanyuan 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.ShiyanyuanView" >

		SELECT  shiyanyuan.* FROM shiyanyuan  shiyanyuan 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.ShiyanyuanView" >
		SELECT * FROM shiyanyuan  shiyanyuan <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
</mapper>